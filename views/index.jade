extends layout

block content
  // banner
  #myCarousel.carousel.slide(data-ride='carousel')
    // Indicators
    ol.carousel-indicators
      li.active(data-target='#myCarousel', data-slide-to='0')
      li(data-target='#myCarousel', data-slide-to='1')
      li(data-target='#myCarousel', data-slide-to='2')
      li(data-target='#myCarousel', data-slide-to='3')
      li(data-target='#myCarousel', data-slide-to='4')
    .carousel-inner(role='listbox')
      .item.active
        .container
          .carousel-caption
            h3
              | The Biggest 
              span Sale
            p Special for today
            a.hvr-outline-out.button2(href='mens.html') Shop Now 
      .item.item2
        .container
          .carousel-caption
            h3
              | Summer 
              span Collection
            p New Arrivals On Sale
            a.hvr-outline-out.button2(href='mens.html') Shop Now 
      .item.item3
        .container
          .carousel-caption
            h3
              | The Biggest 
              span Sale
            p Special for today
            a.hvr-outline-out.button2(href='mens.html') Shop Now 
      .item.item4
        .container
          .carousel-caption
            h3
              | Summer 
              span Collection
            p New Arrivals On Sale
            a.hvr-outline-out.button2(href='mens.html') Shop Now 
      .item.item5
        .container
          .carousel-caption
            h3
              | The Biggest 
              span Sale
            p Special for today
            a.hvr-outline-out.button2(href='mens.html') Shop Now 
    a.left.carousel-control(href='#myCarousel', role='button', data-slide='prev')
      span.glyphicon.glyphicon-chevron-left(aria-hidden='true')
      span.sr-only Previous
    a.right.carousel-control(href='#myCarousel', role='button', data-slide='next')
      span.glyphicon.glyphicon-chevron-right(aria-hidden='true')
      span.sr-only Next
